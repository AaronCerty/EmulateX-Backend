  "scripts": {
    "build": "nest build",
    "start": "nest start",
    "start-stand-alone-app:dev": "ts-node -r tsconfig-paths/register ./src/stand-alone.app",
    "start-stand-alone-app:prod": "node dist/stand-alone.app",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "typeorm:migrate": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:run -d ./src/data-source.ts",
    "typeorm:generate": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:generate -d ./src/data-source.ts src/migrations/syncDb",
    "typeorm:migrate:prod": "node node_modules/typeorm/cli.js migration:run -d ./dist/data-source.js",
    "typeorm:generate:prod": "node node_modules/typeorm/cli.js migration:generate -d ./dist/data-source.js src/migrations/syncDb",
    "typeorm:revert": "typeorm-ts-node-commonjs migration:revert -d ./src/data-source.ts",
    "typeorm:create": "typeorm-ts-node-commonjs migration:create",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix"
  },
  "dependencies": {
    "@liaoliaots/nestjs-redis": "9.0.5",
    "@msgpack/msgpack": "^3.0.0-beta2",
    "@nestjs/axios": "4.0.0",
