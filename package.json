    "@nestjs/throttler": "6.4.0",
    "@nestjs/typeorm": "11.0.0",
    "@nestjs/websockets": "^11.1.0",
    "axios": "1.8.4",
    "bcryptjs": "3.0.2",
    "bignumber.js": "^9.3.0",
    "cache-manager": "6.4.2",
    "class-transformer": "0.5.1",
